{
  "name": "my-app",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "backend": "cd apps/backend && bun",
    "web": "cd apps/web && bun",
    "docs": "cd apps/docs && bun",
    "storybook": "cd apps/storybook && bun",
    "design": "cd packages/design && bun",
    "build": "sst build",
    "deploy": "bun backend db:generate && sst deploy",
    "unlock": "sst unlock",
    "test": "turbo test --continue",
    "test:ci": "turbo test:ci --continue",
    "lint": "oxlint --type-aware --type-check",
    "lint:fix": "oxlint --type-aware --type-check --fix",
    "format": "oxfmt",
    "init": "tsx scripts/init/init",
    "init:stripe": "tsx scripts/init/init-stripe",
    "init:existing": "tsx scripts/init/init-existing",
    "update-readme": "doctoc README.md",
    "get-package-version": "tsx scripts/get-package-version",
    "create:package": "tsx scripts/create-package",
    "postinstall": "sherif --fix",
    "pre-commit": "tsx scripts/pre-commit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@posthog/cli": "0.3.3",
    "@repo/tsconfig": "workspace:*",
    "@types/aws-lambda": "8.10.149",
    "@types/inquirer": "9.0.9",
    "@vitest/ui": "3.1.4",
    "axios": "1.9.0",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
    "doctoc": "2.2.0",
    "husky": "9.1.7",
    "inquirer": "9.2.16",
    "jsdom": "26.1.0",
    "lint-staged": "15.2.2",
    "oxfmt": "0.32.0",
    "oxlint": "1.47.0",
    "oxlint-tsgolint": "0.12.1",
    "sst": "3.17.38",
    "tsx": "4.19.4",
    "turbo": "2.5.3",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.4",
    "zod": "3.25.49"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "bun@1.3.3",
  "patchedDependencies": {
    "ai@5.0.104": "patches/ai@5.0.104.patch"
  }
}
