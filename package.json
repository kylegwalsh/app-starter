{
  "name": "my-app",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "sst dev",
    "backend": "pnpm --filter backend",
    "web": "pnpm --filter web",
    "docs": "pnpm --filter docs",
    "design": "pnpm --filter design",
    "build": "sst build",
    "deploy": "pnpm backend db:generate && sst deploy",
    "lint": "turbo lint",
    "format": "turbo format",
    "init": "tsx scripts/init",
    "update-readme": "doctoc README.md",
    "create:package": "tsx scripts/create-package",
    "postinstall": "manypkg fix",
    "prepare": "husky",
    "precommit": "pnpm lint-staged"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.24.0",
    "@repo/eslint": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@types/aws-lambda": "8.10.149",
    "@vitest/ui": "^3.1.4",
    "axios": "^1.9.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "doctoc": "2.2.0",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "inquirer": "^9.2.16",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "posthog-cli": "^0.0.17",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sst": "3.14.27",
    "tsx": "^4.19.4",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4",
    "zod": "^3.24.2"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm exec eslint --fix",
      "pnpm exec prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "pnpm exec prettier --write"
    ]
  }
}
