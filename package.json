{
  "name": "my-app",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev:vercel": "concurrently \"vercel dev apps/backend --yes\" \"vercel dev apps/web --yes\" --kill-others-on-fail",
    "backend": "cd apps/backend && bun",
    "web": "cd apps/web && bun",
    "docs": "cd apps/docs && bun",
    "storybook": "cd apps/storybook && bun",
    "design": "cd packages/design && bun",
    "build": "turbo build",
    "deploy": "turbo deploy",
    "deploy:prod": "turbo deploy:prod",
    "test": "turbo test --continue",
    "test:ci": "turbo test:ci --continue",
    "lint": "ultracite check .",
    "lint:fix": "ultracite fix .",
    "types:check": "turbo types:check --continue",
    "init": "tsx scripts/init/init",
    "init:stripe": "tsx scripts/init/init-stripe",
    "init:existing": "tsx scripts/init/init-existing",
    "update-readme": "doctoc README.md",
    "get-package-version": "tsx scripts/get-package-version",
    "create:package": "tsx scripts/create-package",
    "postinstall": "manypkg fix",
    "pre-commit": "tsx scripts/pre-commit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@manypkg/cli": "0.24.0",
    "@orpc/client": "1.12.3",
    "@orpc/openapi": "1.12.3",
    "@orpc/server": "1.12.3",
    "@orpc/tanstack-query": "1.12.3",
    "@orpc/zod": "1.12.3",
    "@posthog/cli": "0.3.3",
    "@repo/tsconfig": "workspace:*",
    "@types/inquirer": "9.0.9",
    "@vitest/ui": "3.1.4",
    "axios": "1.9.0",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
    "doctoc": "2.2.0",
    "husky": "9.1.7",
    "inquirer": "9.2.16",
    "jsdom": "26.1.0",
    "lint-staged": "15.2.2",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "tsx": "4.19.4",
    "turbo": "2.5.3",
    "typescript": "5.7.2",
    "ultracite": "6.3.2",
    "vercel": "50.1.6",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.4",
    "zod": "3.25.49"
  },
  "patchedDependencies": {
    "ai@5.0.104": "patches/ai@5.0.104.patch"
  },
  "resolutions": {
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}
