{
  "name": "@repo/docs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo --port 3001",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "pretty": "prettier --write . --ignore-path ../../.prettierignore",
    "format": "pnpm lint:fix & pnpm pretty",
    "types:check": "tsc --noEmit",
    "update-readme": "doctoc README.md",
    "postinstall": "fumadocs-mdx"
  },
  "dependencies": {
    "fumadocs-core": "15.3.2",
    "fumadocs-mdx": "11.6.3",
    "fumadocs-ui": "15.3.2",
    "next": "15.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.7",
    "@types/mdx": "2.0.13",
    "@types/node": "20.17.19",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "postcss": "8.5.4",
    "tailwindcss": "4.1.7",
    "typescript": "5.8.3"
  }
}
