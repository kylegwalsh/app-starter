  "dependencies": {
    "fumadocs-core": "15.3.2",
    "fumadocs-mdx": "11.6.3",
    "fumadocs-ui": "15.3.2",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3"
  }
