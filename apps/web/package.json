    "@repo/config": "workspace:*",
    "@repo/constants": "workspace:*",
    "@repo/design": "workspace:*",
    "@repo/flags": "workspace:*",
    "@repo/seo": "workspace:*",
    "@repo/utils": "workspace:*",
    "@tanstack/react-query": "5.79.0",
    "@trpc/client": "11.2.0",
    "@trpc/react-query": "11.2.0",
    "better-auth": "1.4.2",
    "jotai": "2.12.4",
    "lucide-react": "0.475.0",
    "next": "15.3.6",
    "next-themes": "0.4.6",
    "react": "19.1.0",
