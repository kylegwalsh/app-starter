{
  "name": "@repo/web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true NEXT_PUBLIC_STAGE=dev next build",
    "deploy": "vercel deploy",
    "deploy:prod": "vercel deploy --prod",
    "test": "vitest run",
    "test:ci": "cross-env LOG_LEVEL=silent vitest run --silent",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "ultracite check .",
    "lint:fix": "ultracite fix .",
    "types:check": "bun --cwd ../backend types:generate && tsc --noEmit",
    "update-readme": "doctoc README.md",
    "postinstall": "tsx scripts/install-playwright"
  },
  "dependencies": {
    "@daveyplate/better-auth-ui": "3.0.2",
