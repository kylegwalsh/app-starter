{
  "name": "@app/web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "sst shell next build",
    "dev": "sst shell next dev",
    "start": "sst shell next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "concurrently \"pnpm run test:unit\" \"pnpm run test:e2e\"",
    "test:unit": "sst shell vitest run",
    "test:unit:ui": "sst shell vitest --ui",
    "test:e2e": "sst shell playwright test",
    "test:e2e:ui": "sst shell playwright test --ui",
    "postinstall": "npx playwright install chromium --with-deps"
  },
  "dependencies": {
    "@lib/analytics": "workspace:*",
    "@lib/config": "workspace:*",
    "@lib/constants": "workspace:*",
    "@lib/ui": "workspace:*",
    "@lib/utils": "workspace:*",
    "@tanstack/react-query": "^5.76.1",
    "@trpc/client": "^11.1.2",
    "@trpc/react-query": "^11.1.2",
    "jotai": "^2.12.4",
    "lucide-react": "^0.475.0",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "superjson": "^2.2.2"
  },
  "devDependencies": {
    "@lib/eslint": "workspace:*",
    "@lib/tsconfig": "workspace:*",
    "@playwright/test": "^1.52.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3"
  }
}
