    "@playwright/test": "1.52.0",
    "@repo/backend": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@trpc/server": "11.2.0",
    "@types/node": "24.10.1",
    "@types/react": "19.1.4",
