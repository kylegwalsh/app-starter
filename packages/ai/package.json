{
  "name": "@repo/ai",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
